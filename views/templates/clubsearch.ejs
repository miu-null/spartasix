<form class="clubs-list">
    <h3 style="text-align: center; margin: 50px 0px 50px 50px;">스파르타커뮤클럽</h3>
    <div class="form-club">
      <div class="form-category">
        <p>모각코/모임 게시판</p>
      </div>
      <div class="post-btn">
        <button type="button" class="club-post" onclick="location.href='/club/clubspost'">글 쓰기</button>
      </div>
  
  
      <table class="table table-hover align-middle text-center">
        <thead class="post-btn">
            <tr>
  
              <th scope="col">글번호</th>
              <th scope="col">회원번호</th>
              <th scope="col">제목</th>
              <th scope="col">내용</th>
              <th scope="col">신청</th>
            </tr>
          </thead>
      <tbody>
        <% slicedData.forEach(data => { %>
        <tr>
          <td><%= data.clubId %></td>
          <td><%= data.userId %></td>
          <td><%= data.title %></td>
          <td><%= data.content %></td></a></td>
          <td><a class="btn btn-outline-secondary" onclick="location.href='list/<%= data.clubId%>'" role="button">자세히</a></td>
        </tr>
        <% }) %>
      </tbody>
      </table>
  
  
    </div>
  </form>
  
  <!-- 페이지네이션 버튼 -->
  <div id="pagination">
    <nav aria-label="Page navigation">
      <ul class="pagination justify-content-center">
        <% if (unitStart - 1> 0 ) { %>
          <li class="page-item">
            <a class="page-link" href="list?term=<%=terms.term %>&page=<%= unitStart -1 %>" tabindex="-1"
              aria-disabled="true">이전</span>
            </a>
          </li>
          <% } %>
            <% for (let i=unitStart; i <= unitEnd; i++) { %>
              <% if (i <= lastPage) { %>
                <% if ( i===page) { %>
                  <li class="page-item active" aria-current="page">
                    <span class="page-link">
                      <%= i %>
                    </span>
                  </li>
                  <% } else { %>
                    <li class="page-item"><a class="page-link" href="list?term=<%=term.term %>&page=<%= i %>">
                        <%= i %>
                      </a></li>
                    <% } %>
                      <% } %>
                        <% } %>
                          <% if (unitEnd <=lastPage) { %>
                            <li class="page-item">
                              <a class="page-link" href="list?term=<%=term.term %>&page=<%= unitEnd +1 %>">다음
                              </a>
                            </li>
                            <% } %> 
      </ul>
    </nav>
  </div>
  
  
  
  <section id="search" class="search-wrap">
    <form action="/club/list" class="search-form">
      <!-- <div class="col-10 col-lg-auto mb-3 mb-lg-0 me-lg-3"> -->
      <input type="search" id="term" name="term" class="form-control" placeholder="🔍(테스트)게시판 검색 : 제목, 내용"
        aria-label="Search" pattern=".{1}|.{1,10}" required title="최소 1자 이상 입력해야 합니다">
    </form>
  </section>
  