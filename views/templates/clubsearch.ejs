<div id="pagination">
  <nav aria-label="Page navigation">
    <ul class="pagination justify-content-center">
      <% if (unitStart - 1> 0 ) { %>
        <li class="page-item">
          <a class="page-link" href="search?term=<%=term.term %>&page=<%= unitStart -1 %>" tabindex="-1"
            aria-disabled="true">Ïù¥Ï†Ñ</span>
          </a>
        </li>
        <% } %>
          <% for (let i=unitStart; i <= unitEnd; i++) { %>
            <% if (i <= lastPage) { %>
              <% if ( i===page) { %>
                <li class="page-item active" aria-current="page">
                  <span class="page-link">
                    <%= i %>
                  </span>
                </li>
                <% } else { %>
                  <li class="page-item"><a class="page-link" href="search?term=<%=term.term %>&page=<%= i %>">
                      <%= i %>
                    </a></li>
                  <% } %>
                    <% } %>
                      <% } %>
                        <% if (unitEnd <=lastPage) { %>
                          <li class="page-item">
                            <a class="page-link" href="search?term=<%=term.term %>&page=<%= unitEnd +1 %>">Îã§Ïùå
                            </a>
                          </li>
                          <% } %> 
    </ul>
  </nav>
</div>

