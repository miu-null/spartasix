<div class="container club_container">
  <div>
    <a href="clubspost" class="new_club">+ new post</a>
  </div>
  <div class="club_table_container">
    <table class="table table-hover club_table">
      <thead id="club_thead">
        <tr>
          <th>Club</th>
          <th>Title</th>
          <th>Writer</th>
          <th>maxMembers</th>
          <th>Date</th>
          <th>View</th>
          <th>Likes</th>
        </tr>
      </thead>
      <tbody>
        <% slicedData.forEach(data => { %>
          <tr id="club_tr">
            <td id="club_Id"><%= data.clubId %></td>
            <td id="club_Title"><a class="postLink" href='<%= data.Id%>'><%= data.title %></a></td>
            <td id="club_Writer"><a class="postLink" href="/userpage/<%= data.userId %>"><%= data.user.nickName %></td>
            <td id="club_maxMember"><%= data.maxMembers %></td>
            <td id="club_Date"><%= data.createdAt %></td>
            <td id="club_View"><%= data.viewCount %></td>
            <td id="club_Likes"><%= data.Likes %></td>
          </tr>
          <% }) %>
      </tbody>
    </table>
  </div>
</div>
  
