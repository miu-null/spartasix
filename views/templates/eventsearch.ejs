<div class="container event_container">
  <div>
      <a href="newevent" class="new_event">+ new post</a>
  </div>
  <div class="event_table_container">
      <table class="table table-hover event_table">
          <thead id="event_thead">
              <tr>
                  <th>Event</th>
                  <th>Title</th>
                  <th>Writer</th>
                  <th>Event Date</th>
                  <th>View</th>
                  <th>Likes</th>
              </tr>
          </thead>
          <tbody>
              <% slicedData.forEach(data => { %>
              <tr>
                <td><a class="postLink" href='list/<%= data.eventPostId%>'><%= data.eventPostId %></a></td>
                <td><a class="postLink" href='list/<%= data.eventPostId%>'><%= data.title %></a></td>
                <td><a class="postLink" href="/userpage/<%= data.userId %>"><%= data.user.nickName %></a></td>
                <td><%= data.createdateAt %></td>
                <td><%= data.viewCount %></td>
                <td><%= data.Likes %></td>
              </tr>
              <% }) %>
            </tbody>
      </table>
  </div>
</div>
  
  <!-- ÌéòÏù¥ÏßÄÎÑ§Ïù¥ÏÖò Î≤ÑÌäº -->
  <div id="pagination">
    <nav aria-label="Page navigation">
      <ul class="pagination justify-content-center">
        <% if (unitStart - 1> 0 ) { %>
          <li class="page-item">
            <a class="page-link" href="search?&page=<%= unitStart -1 %>" tabindex="-1"
              aria-disabled="true">Ïù¥Ï†Ñ</span>
            </a>
          </li>
          <% } %>
            <% for (let i=unitStart; i <= unitEnd; i++) { %>
              <% if (i <= lastPage) { %>
                <% if ( i===page) { %>
                  <li class="page-item active" aria-current="page">
                    <span class="page-link">
                      <%= i %>
                    </span>
                  </li>
                  <% } else { %>
                    <li class="page-item"><a class="page-link" href="search?term=<%=term.term %>&page=<%= i %>">
                        <%= i %>
                      </a></li>
                    <% } %>
                      <% } %>
                        <% } %>
                          <% if (unitEnd <=lastPage) { %>
                            <li class="page-item">
                              <a class="page-link" href="search?term=<%=term.term %>&page=<%= unitEnd +1 %>">Îã§Ïùå
                              </a>
                            </li>
                            <% } %> 
      </ul>
    </nav>
  </div>
  
  
  
