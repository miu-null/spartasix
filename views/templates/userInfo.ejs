  <script>
        $(document).ready(function(){
          checkClubs(`<%= myInfo.userId %>`)
        });
        
        // Function to load the next page of posts
        const loadNextPage = () => {
          show_userPosts(`<%= myInfo.userId %>`, startCursor, endCursor, limit);
          let startCursor = 0;
          let endCursor = 0;
          const limit = 3; // or whatever limit you want to use
        };
        
    // Event listener for "next page" button
    document.getElementById("nextClubBtn").addEventListener("click", () => {
      startCursor = endCursor;
      limit = 3;
      loadNextPage();
    });
    console.log(startCursor)
    console.log("sleepy")
    // Event listener for "previous page" button
    document.getElementById("prevClubBtn").addEventListener("click", () => {
      endCursor = startCursor;
      startCursor = null;
      limit = 3;
      loadNextPage();
    });
  
    // Load the first page of posts
    loadNextPage();
    </script>

