<div class="eventdetail_container">
  <div class="eventdetail_button">
    <div class="eventdetail_edit">
      <a style="cursor: pointer" onclick="clubupdate()"
        class="new_event">수정&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp</a>
    </div>
    <div class="eventdetail_delete">
      <a style="cursor: pointer" href="/club/list" class="new_event">취소</a>
    </div>
  </div>
  <div class="eventdetail_title">
    <div class="eventdetail_first">
      <div class="eventdetail_first_title">
        <div class="first_title">
          &nbsp;&nbsp;&nbsp;&nbsp;Title
        </div>
        <div class="title_text">
          <input id="club_title" class="event_title" placeholder="title">
        </div>
        <div class="club_category">
          category
          <select id="club_category" class="form-select" aria-label="Default select example">
            <option selected>카테고리</option>
            <option value="1">공부</option>
            <option value="2">프로젝트</option>
            <option value="3">기타</option>
          </select>
        </div>
      </div>
    </div>
    <div class="eventdetail_second">
      <div class="eventdetail_second_start">
        <div class="start_date">
          maxMembers :
        </div>
        <div class="col-3">
          <input id="club_maxMembers" class="event_title" placeholder="maxMember">
        </div>
      </div>
    </div>
  </div>
  <div class="eventdetail_body">
    <div class="eventdetail_body_text">
      <textarea id="club_content" placeholder="content" class="textarea"></textarea>
    </div>
  </div>